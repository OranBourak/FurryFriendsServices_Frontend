{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "google",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": [

            "error",

            4

        ],

        "quotes": [

            "error",

            "double"

        ],
        "max-len": "off"

    }
}
